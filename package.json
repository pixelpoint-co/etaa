    "eslint-config-airbnb": "19.0.4",
    "eslint-plugin-destructuring-newline": "0.0.2",
    "gh-pages": "4.0.0",
    "sass": "1.53.0",
    "storybook-addon-styled-component-theme": "2.0.0",
    "webpack": "5.74.0"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
