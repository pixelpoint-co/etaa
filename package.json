  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:clear-cache": "start-storybook --no-manager-cache",
    "svg": "svgo --folder ./src/components/atoms/Icon/icons -o ./src/components/atoms/Icon/icons-o",
    "build-storybook": "build-storybook -s public",
    "build": "react-scripts build",
    "build:staging": "env-cmd -f ./.env.staging react-scripts build",
    "build:page": "NODE_ENV=development react-scripts build",
    "predeploy:page": "npm run build:page",
    "deploy:page": "gh-pages -d build"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "6.5.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/addon-interactions": "6.5.9",
